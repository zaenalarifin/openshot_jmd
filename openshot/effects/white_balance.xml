<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE openshot-effect>
<effect>
	<title translatable="True">White Balance</title>
	<description translatable="True">White Balance Description</description>
	<icon>white_balance.png</icon>
	<category>Video</category>
	<service>frei0r.balanc0r</service>
	
	<param name="Neutral Color" type="color" title="Neutral Color" description="Choose a color from the source image that should be white.">
		<default>#e5e5e5</default>
	</param>
	
	<param name="Green Tint" type="spinner" title="Green Tint" description="">
		<min>0.01</min>
		<max>5.0</max>
		<default>1.1</default>
	</param>

</effect>
